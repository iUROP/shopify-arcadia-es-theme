    <div class="i-section-compare-versus__product">
      <div class="compare-versus__header">
        <img src="{{ section.settings.banner_right | image_url: width: 1600 }}" alt="Product Arcadia" />
      </div>
    </div>
  </div>

  <div class="i-section-compare-versus-comparative">
    {% for block in section.blocks %}
      {% case block.type %}
        {% when 'especificacion' %}
          <div class="i-section-compare-versus-comparative__item">

            <div class="i-section-compare-versus-comparative__item--row is-left">
              <div class="i-section-compare-versus-comparative__item--row__progress" style="width: {{ block.settings.left_percent }}%"></div>
            </div>
            <div class="i-section-compare-versus-comparative__item--col Text-Colors-800 Family-Font---Body Typo-Body-M">
              {{ block.settings.title }}
            </div>
            <div class="i-section-compare-versus-comparative__item--row is-right">
              <div class="i-section-compare-versus-comparative__item--row__progress" style="width: {{ block.settings.right_percent }}%"></div>
            </div>

          </div>
      {% endcase %}
    {% endfor %}
  </div>

  <div class="i-section-compare-versus-footer Text-Colors-800 Family-Font---Body Typo-Body-L">
    * Por favor, elige con cuidado
  </div>
    
</div>

