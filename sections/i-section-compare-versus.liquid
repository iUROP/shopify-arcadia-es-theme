<div class="i-section" data-aos="fade-up">
  <div class="i-section-compare-versus">
    <div class="i-section-compare-versus__product">
      <div class="compare-versus__header">
        <img src="{{ section.settings.banner_left | image_url: width: 1600 }}" alt="Product Arcadia" />
      </div>
    </div>


